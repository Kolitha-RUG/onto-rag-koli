@prefix : <http://www.sws.org/sws#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix esco: <http://data.europa.eu/esco/model#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa#> .
@base <http://www.sws.org/sws> .

<http://www.sws.org/sws> rdf:type owl:Ontology ;
                          owl:versionIRI <http://www.sws.org/sws/1.1.7> ;
                          rdfs:label "sws ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty ;
             rdfs:label "comment" .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/1999/02/22-rdf-syntax-ns#langString
rdf:langString rdf:type rdfs:Datatype ;
               rdfs:label "langString" .


###  http://www.w3.org/2001/XMLSchema#double
xsd:double rdf:type rdfs:Datatype ;
           rdfs:label "double" .


###  http://www.w3.org/2001/XMLSchema#int
xsd:int rdf:type rdfs:Datatype ;
        rdfs:label "int" .


###  http://www.w3.org/2001/XMLSchema#integer
xsd:integer rdf:type rdfs:Datatype ;
            rdfs:label "integer" .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype ;
           rdfs:label "string" .


###  http://www.w3.org/2002/07/owl#real
owl:real rdf:type rdfs:Datatype ;
         rdfs:label "real" .


#################################################################
#    Object Properties
#################################################################

###  http://www.sws.org/sws#actorHasPerformanceDimension
:actorHasPerformanceDimension rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :hasPerformanceDimension ;
                              rdfs:domain :Actor ;
                              rdfs:range :Performance ;
                              rdfs:label "actorHasPerformanceDimension" .


###  http://www.sws.org/sws#assignedTo
:assignedTo rdf:type owl:ObjectProperty ;
            rdfs:domain :Task ;
            rdfs:range :Actor ;
            rdfs:label "assignedTo" .


###  http://www.sws.org/sws#confidenceAssessmentHas
:confidenceAssessmentHas rdf:type owl:ObjectProperty ;
                         rdfs:label "confidenceAssessmentHas" .


###  http://www.sws.org/sws#confidenceAssessmentHasActor
:confidenceAssessmentHasActor rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :confidenceAssessmentHas ;
                              rdfs:domain :ConfidenceAssessment ;
                              rdfs:range :Actor ;
                              rdfs:label "confidenceAssessmentHasActor" .


###  http://www.sws.org/sws#confidenceAssessmentHasTask
:confidenceAssessmentHasTask rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :confidenceAssessmentHas ;
                             rdfs:domain :ConfidenceAssessment ;
                             rdfs:range :Task ;
                             rdfs:label "confidenceAssessmentHasTask" .


###  http://www.sws.org/sws#hasActorStatus
:hasActorStatus rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasStatus ;
                rdfs:domain :Actor ;
                rdfs:range :ActorStatus ;
                rdfs:label "hasActorStatus" .


###  http://www.sws.org/sws#hasConstraint
:hasConstraint rdf:type owl:ObjectProperty ;
               rdfs:range :Constraint ;
               rdfs:label "hasConstraint" .


###  http://www.sws.org/sws#hasDecisionStatus
:hasDecisionStatus rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasStatus ;
                   rdfs:domain :Decision ;
                   rdfs:range :DecisionStatus ;
                   rdfs:label "hasDecisionStatus" .


###  http://www.sws.org/sws#hasDigitalTwin
:hasDigitalTwin rdf:type owl:ObjectProperty ;
                rdfs:domain :Actor ;
                rdfs:range :DigitalTwin ;
                rdfs:label "hasDigitalTwin" .


###  http://www.sws.org/sws#hasPerformanceDimension
:hasPerformanceDimension rdf:type owl:ObjectProperty ;
                         rdfs:range :Performance ;
                         rdfs:label "hasPerformanceDimension" .


###  http://www.sws.org/sws#hasSkill
:hasSkill rdf:type owl:ObjectProperty ;
          rdfs:domain :Actor ;
          rdfs:range esco:Skill ;
          rdfs:label "hasSkill" .


###  http://www.sws.org/sws#hasStatus
:hasStatus rdf:type owl:ObjectProperty ;
           rdfs:label "hasStatus" .


###  http://www.sws.org/sws#hasTask
:hasTask rdf:type owl:ObjectProperty ;
         rdfs:domain :Schedule ;
         rdfs:range :Task ;
         rdfs:label "hasTask" .


###  http://www.sws.org/sws#hasTaskStatus
:hasTaskStatus rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasStatus ;
               rdfs:domain :Task ;
               rdfs:range :TaskStatus ;
               rdfs:label "hasTaskStatus" .


###  http://www.sws.org/sws#isAllocatedTo
:isAllocatedTo rdf:type owl:ObjectProperty ;
               rdfs:domain :Resource ;
               rdfs:range :Task ;
               rdfs:label "isAllocatedTo" .


###  http://www.sws.org/sws#isObservedBy
:isObservedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :observes ;
              rdfs:domain :NonHumanActor ;
              rdfs:range sosa:Sensor ;
              rdfs:label "isObservedBy" .


###  http://www.sws.org/sws#monitorsActor
:monitorsActor rdf:type owl:ObjectProperty ;
               rdfs:domain :DigitalTwin ;
               rdfs:range :Actor ;
               rdfs:label "monitorsActor" .


###  http://www.sws.org/sws#observes
:observes rdf:type owl:ObjectProperty ;
          rdfs:domain sosa:Sensor ;
          rdfs:range :NonHumanActor ;
          rdfs:label "observes" .


###  http://www.sws.org/sws#requiresResource
:requiresResource rdf:type owl:ObjectProperty ;
                  rdfs:domain :Task ;
                  rdfs:range :Resource ;
                  rdfs:label "requiresResource" .


###  http://www.sws.org/sws#requiresSkill
:requiresSkill rdf:type owl:ObjectProperty ;
               rdfs:domain :Task ;
               rdfs:range esco:Skill ;
               rdfs:label "requiresSkill" .


###  http://www.sws.org/sws#scheduleHasConstraint
:scheduleHasConstraint rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasConstraint ;
                       rdfs:domain :Schedule ;
                       rdfs:range :Constraint ;
                       rdfs:label "scheduleHasConstraint" .


###  http://www.sws.org/sws#scheduleHasPerformanceDimension
:scheduleHasPerformanceDimension rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :hasPerformanceDimension ;
                                 rdfs:domain :Schedule ;
                                 rdfs:range :Performance ;
                                 rdfs:label "scheduleHasPerformanceDimension" .


###  http://www.sws.org/sws#taskHasConstraint
:taskHasConstraint rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasConstraint ;
                   rdfs:domain :Task ;
                   rdfs:range :Constraint ;
                   rdfs:label "taskHasConstraint" .


###  http://www.sws.org/sws#taskHasPerformanceDimension
:taskHasPerformanceDimension rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasPerformanceDimension ;
                             rdfs:domain :Task ;
                             rdfs:range :Performance ;
                             rdfs:label "taskHasPerformanceDimension" .


###  http://www.sws.org/sws#triggersSliding
:triggersSliding rdf:type owl:ObjectProperty ;
                 rdfs:domain :Context ;
                 rdfs:range :Schedule ;
                 rdfs:label "triggersSliding" .


###  http://www.sws.org/sws#usesResource
:usesResource rdf:type owl:ObjectProperty ;
              rdfs:domain :Actor ;
              rdfs:range :Resource ;
              rdfs:label "usesResource" .


#################################################################
#    Data properties
#################################################################

###  http://www.sws.org/sws#hasActorRole
:hasActorRole rdf:type owl:DatatypeProperty ;
              rdfs:domain :Actor ;
              rdfs:range xsd:string ;
              rdfs:label "hasActorRole" .


###  http://www.sws.org/sws#hasActorStatusDescriptor
:hasActorStatusDescriptor rdf:type owl:DatatypeProperty ;
                          rdfs:domain :ActorStatus ;
                          rdfs:range xsd:string ;
                          rdfs:label "hasActorStatusDescriptor" .


###  http://www.sws.org/sws#hasConfidence
:hasConfidence rdf:type owl:DatatypeProperty ;
               rdfs:domain :ConfidenceAssessment ;
               rdfs:range xsd:decimal ;
               rdfs:label "hasConfidence" .


###  http://www.sws.org/sws#hasDecisionStatusDescriptor
:hasDecisionStatusDescriptor rdf:type owl:DatatypeProperty ;
                             rdfs:domain :DecisionStatus ;
                             rdfs:range xsd:string ;
                             rdfs:label "hasDecisionStatusDescriptor" .


###  http://www.sws.org/sws#hasOperationalPerformanceScore
:hasOperationalPerformanceScore rdf:type owl:DatatypeProperty ;
                                rdfs:subPropertyOf :hasPerformanceScore ;
                                rdfs:domain :Operational ;
                                rdfs:range xsd:decimal ;
                                rdfs:label "hasOperationalPerformanceScore" .


###  http://www.sws.org/sws#hasPerformanceScore
:hasPerformanceScore rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Performance ;
                     rdfs:range xsd:decimal ;
                     rdfs:label "hasPerformanceScore" .


###  http://www.sws.org/sws#hasPriorityLevel
:hasPriorityLevel rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Task ;
                  rdfs:range xsd:int ;
                  rdfs:label "hasPriorityLevel" .


###  http://www.sws.org/sws#hasSensorValue
:hasSensorValue rdf:type owl:DatatypeProperty ;
                rdfs:domain sosa:Sensor ;
                rdfs:range owl:real ;
                rdfs:label "hasSensorValue" .


###  http://www.sws.org/sws#hasSkillLevel
:hasSkillLevel rdf:type owl:DatatypeProperty ;
               rdfs:domain :Actor ;
               rdfs:range xsd:integer ;
               rdfs:label "hasSkillLevel" .


###  http://www.sws.org/sws#hasSocialPerformanceScore
:hasSocialPerformanceScore rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :hasPerformanceScore ;
                           rdfs:domain :Social ;
                           rdfs:range xsd:decimal ;
                           rdfs:label "hasSocialPerformanceScore" .


###  http://www.sws.org/sws#hasTaskComplexity
:hasTaskComplexity rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Task ;
                   rdfs:range xsd:string ;
                   rdfs:label "hasTaskComplexity" .


###  http://www.sws.org/sws#hasTaskStatusDescriptor
:hasTaskStatusDescriptor rdf:type owl:DatatypeProperty ;
                         rdfs:domain :TaskStatus ;
                         rdfs:range xsd:string ;
                         rdfs:label "hasTaskStatusDescriptor" .


###  http://www.sws.org/sws#hasTechnicalPerformanceScore
:hasTechnicalPerformanceScore rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf :hasPerformanceScore ;
                              rdfs:domain :Technical ;
                              rdfs:range xsd:decimal ;
                              rdfs:label "hasTechnicalPerformanceScore" .


#################################################################
#    Classes
#################################################################

###  http://data.europa.eu/esco/model#Skill
esco:Skill rdf:type owl:Class ;
           rdfs:comment "A capability or competence possessed by an actor, based on the ESCO taxonomy." ;
           rdfs:label "Skill" .


###  http://www.sws.org/sws#AIAcceptance
:AIAcceptance rdf:type owl:Class ;
              rdfs:subClassOf :Social ;
              rdfs:comment "AI acceptance measurements quantify the degree to which human actors (usually workers) accept the AI agents’ presence in a work environment." ;
              rdfs:label "AIAcceptance" .


###  http://www.sws.org/sws#Accuracy
:Accuracy rdf:type owl:Class ;
          rdfs:subClassOf :Technical ;
          rdfs:comment "Accuracy meatrics measure the degree to which an AI model generates a correct output for the problem given the available data." ;
          rdfs:label "Accuracy" .


###  http://www.sws.org/sws#Actor
:Actor rdf:type owl:Class ;
       rdfs:comment "Represents an entity capable of performing tasks or making decisions." ;
       rdfs:label "Actor" .


###  http://www.sws.org/sws#ActorStatus
:ActorStatus rdf:type owl:Class ;
             rdfs:subClassOf :Status ;
             rdfs:label "ActorStatus" .


###  http://www.sws.org/sws#Agency
:Agency rdf:type owl:Class ;
        rdfs:subClassOf :Social ;
        rdfs:comment "Agency metrics traditionally measure the discretion of human agents when making decisions to carry out work (human agency)." ;
        rdfs:label "Agency" .


###  http://www.sws.org/sws#ConfidenceAssessment
:ConfidenceAssessment rdf:type owl:Class ;
                      rdfs:label "ConfidenceAssessment" .


###  http://www.sws.org/sws#Constraint
:Constraint rdf:type owl:Class ;
            rdfs:comment "Defines any limitation, requirement, or boundary condition that affects task execution and must not be violated by a schedule." ;
            rdfs:label "Constraint" .


###  http://www.sws.org/sws#Context
:Context rdf:type owl:Class ;
         rdfs:comment "Any information that can be used to characterize the situation of an entity." ;
         rdfs:label "Context" .


###  http://www.sws.org/sws#ContinuantResource
:ContinuantResource rdf:type owl:Class ;
                    rdfs:subClassOf :Resource ;
                    owl:disjointWith :OccurrentResource ;
                    rdfs:comment "Resources that continue to persist through time while maintaining their identity. (IFO/BFO)" ;
                    rdfs:label "ContinuantResource" .


###  http://www.sws.org/sws#CostBenefitRatios
:CostBenefitRatios rdf:type owl:Class ;
                   rdfs:subClassOf :Operational ;
                   rdfs:comment "The cost/benefit ratios are measures associating the costs relative to the benefits of a process and are indicative of the potential profit margins of operations." ;
                   rdfs:label "CostBenefitRatios" .


###  http://www.sws.org/sws#Costs
:Costs rdf:type owl:Class ;
       rdfs:subClassOf :Operational ;
       rdfs:comment "Cost metrics measure the costs associated with a process." ;
       rdfs:label "Costs" .


###  http://www.sws.org/sws#CycleTime
:CycleTime rdf:type owl:Class ;
           rdfs:subClassOf :Operational ;
           rdfs:comment "Cycle time indicators measure the elapsed time between initiating and completing a task, process, or machine operation." ;
           rdfs:label "CycleTime" .


###  http://www.sws.org/sws#Decision
:Decision rdf:type owl:Class ;
          rdfs:comment "Representing the conclusion or choice among alternatives to allocate tasks, adjust schedules, or adaptive strategies" ;
          rdfs:label "Decision" .


###  http://www.sws.org/sws#DecisionStatus
:DecisionStatus rdf:type owl:Class ;
                rdfs:subClassOf :Status ;
                rdfs:label "DecisionStatus" .


###  http://www.sws.org/sws#DeliveryReliability
:DeliveryReliability rdf:type owl:Class ;
                     rdfs:subClassOf :Operational ;
                     rdfs:comment "Delivery reliability metrics measure a supplier’s capacity to serve customers on time." ;
                     rdfs:label "DeliveryReliability" .


###  http://www.sws.org/sws#DependentContinuants
:DependentContinuants rdf:type owl:Class ;
                      rdfs:subClassOf :ContinuantResource ;
                      rdfs:comment "cannot exist without an independent continuant. (qualities of a product)" ;
                      rdfs:label "DependentContinuants" .


###  http://www.sws.org/sws#DeploymentCosts
:DeploymentCosts rdf:type owl:Class ;
                 rdfs:subClassOf :Operational ;
                 rdfs:comment "Indicators measuring the cost of deploying a solution in an operational environment." ;
                 rdfs:label "DeploymentCosts" .


###  http://www.sws.org/sws#DerivedContext
:DerivedContext rdf:type owl:Class ;
                rdfs:subClassOf :Context ;
                rdfs:comment "Context inferred from raw data or calculated through processing, reasoning, or interpretations. (e.g., Workload)" ;
                rdfs:label "DerivedContext" .


###  http://www.sws.org/sws#DigitalTwin
:DigitalTwin rdf:type owl:Class ;
             rdfs:comment "The real-time digital representation of a physical actor, environment, or process." ;
             rdfs:label "DigitalTwin" .


###  http://www.sws.org/sws#DigitalTwinDecision
:DigitalTwinDecision rdf:type owl:Class ;
                     rdfs:subClassOf :Decision ;
                     rdfs:comment "A decision made by DigitalTwin" ;
                     rdfs:label "DigitalTwinDecision" .


###  http://www.sws.org/sws#EaseOfUse
:EaseOfUse rdf:type owl:Class ;
           rdfs:subClassOf :Social ;
           rdfs:comment "Ease of use metrics measure the accessibility of a system’s functionalities to a user and the ease of learning to use the system effectively and efficiently." ;
           rdfs:label "EaseOfUse" .


###  http://www.sws.org/sws#EnvironmentalSustainability
:EnvironmentalSustainability rdf:type owl:Class ;
                             rdfs:subClassOf :Operational ;
                             rdfs:comment "Environmental sustainability metrics measure the environmental impact of a process." ;
                             rdfs:label "EnvironmentalSustainability" .


###  http://www.sws.org/sws#Explainability
:Explainability rdf:type owl:Class ;
                rdfs:subClassOf :Social ;
                rdfs:comment "Explainability refers to the capacity of a system to provide humans with comprehensible explanations of its decisions and the rationale of its actions (outcome-centric understandability)." ;
                rdfs:label "Explainability" .


###  http://www.sws.org/sws#Fairness
:Fairness rdf:type owl:Class ;
          rdfs:subClassOf :Social ;
          rdfs:comment "Fairness assessments quantify a system’s non-discrimination capabilities." ;
          rdfs:label "Fairness" .


###  http://www.sws.org/sws#FeedbackFromOthers
:FeedbackFromOthers rdf:type owl:Class ;
                    rdfs:subClassOf :Social ;
                    rdfs:comment "Feedback metrics measure the degree of feedback provided to a worker in a work environment." ;
                    rdfs:label "FeedbackFromOthers" .


###  http://www.sws.org/sws#Flexibility
:Flexibility rdf:type owl:Class ;
             rdfs:subClassOf :Operational ;
             rdfs:comment "Flexibility measures capture the degree to which processes can be modified to address changes in operational requirements." ;
             rdfs:label "Flexibility" .


###  http://www.sws.org/sws#HardConstraint
:HardConstraint rdf:type owl:Class ;
                rdfs:subClassOf :Constraint ;
                rdfs:comment "A non-negotiable constraint that must always be satisfied by any schedule" ;
                rdfs:label "HardConstraint" .


###  http://www.sws.org/sws#HumanActor
:HumanActor rdf:type owl:Class ;
            rdfs:subClassOf :Actor ;
            owl:disjointWith :NonHumanActor ;
            rdfs:comment "A subclass of Actor representing a human entity involved in performing tasks or making decisions" ;
            rdfs:label "HumanActor" .


###  http://www.sws.org/sws#HumanDecision
:HumanDecision rdf:type owl:Class ;
               rdfs:subClassOf :Decision ;
               rdfs:comment "A decision made solely by a human actor" ;
               rdfs:label "HumanDecision" .


###  http://www.sws.org/sws#Inclusivity
:Inclusivity rdf:type owl:Class ;
             rdfs:subClassOf :Social ;
             rdfs:comment "Inclusivity indicators quantify the practice of providing equal access to opportunities and resources for users, especially users belonging to minority groups or traditionally misrepresented groups." ;
             rdfs:label "Inclusivity" .


###  http://www.sws.org/sws#IndependentContinuants
:IndependentContinuants rdf:type owl:Class ;
                        rdfs:subClassOf :ContinuantResource ;
                        rdfs:comment "Do not depend on other resources for their existence. (e.g., person, machine, product)" ;
                        rdfs:label "IndependentContinuants" .


###  http://www.sws.org/sws#InformationProcessingNeeds
:InformationProcessingNeeds rdf:type owl:Class ;
                            rdfs:subClassOf :Social ;
                            rdfs:comment "Information processing measures quantify the requirements of interpreting, gathering, and synthesising information for decision-making purposes." ;
                            rdfs:label "InformationProcessingNeeds" .


###  http://www.sws.org/sws#Interdependence
:Interdependence rdf:type owl:Class ;
                 rdfs:subClassOf :Social ;
                 rdfs:comment "Interdependence indicators measure the degree to which a process' (human) actors depend on each other." ;
                 rdfs:label "Interdependence" .


###  http://www.sws.org/sws#Interpretability
:Interpretability rdf:type owl:Class ;
                  rdfs:subClassOf :Social ;
                  rdfs:comment "Interpretability metrics measure the ability to translate a system’s working principles into human-understandable language (model design-centric understandability)." ;
                  rdfs:label "Interpretability" .


###  http://www.sws.org/sws#JointDecision
:JointDecision rdf:type owl:Class ;
               rdfs:subClassOf :Decision ;
               rdfs:comment "A decision made through collaboration between human and non-human actors" ;
               rdfs:label "JointDecision" .


###  http://www.sws.org/sws#Latency
:Latency rdf:type owl:Class ;
         rdfs:subClassOf :Technical ;
         rdfs:comment "Latency measures a system’s responsiveness, typically in terms of the time it takes to process one unit of data." ;
         rdfs:label "Latency" .


###  http://www.sws.org/sws#MentalDemands
:MentalDemands rdf:type owl:Class ;
               rdfs:subClassOf :Social ;
               rdfs:comment "Mental demands measurements quantify the cognitive costs associated with performing a task." ;
               rdfs:label "MentalDemands" .


###  http://www.sws.org/sws#NonDerivedContext
:NonDerivedContext rdf:type owl:Class ;
                   rdfs:subClassOf :Context ;
                   rdfs:comment "Raw or directly observable contextual information (e.g., Temperature, Heart Rate)" ;
                   rdfs:label "NonDerivedContext" .


###  http://www.sws.org/sws#NonHumanActor
:NonHumanActor rdf:type owl:Class ;
               rdfs:subClassOf :Actor ;
               rdfs:comment "A subclass of Actor representing artificial or automated entities, such as robots, AI or software, capable of performing tasks or making decisions." ;
               rdfs:label "NonHumanActor" .


###  http://www.sws.org/sws#OccurrentResource
:OccurrentResource rdf:type owl:Class ;
                   rdfs:subClassOf :Resource ;
                   rdfs:comment """Resources occur, happen or unfold in time. (IFO/BFO)

Occurrents can be entities that
1. are a part of time, such as a time instant or time period
2. exist in time by occurring or happening such as a manufacturing process
3. represent a boundary of an occurrence or happening such as the beginning of a process.

ref:The Industrial Ontologies Foundry (IOF) Core Ontology""" ;
                   rdfs:label "OccurrentResource" .


###  http://www.sws.org/sws#Operational
:Operational rdf:type owl:Class ;
             rdfs:subClassOf :Performance ;
             rdfs:comment "Operational evaluation metrics are used to measure the economic performance of operational units." ;
             rdfs:label "Operational" .


###  http://www.sws.org/sws#PerceivedQuality
:PerceivedQuality rdf:type owl:Class ;
                  rdfs:subClassOf :Operational ;
                  rdfs:comment "Perceived quality metrics measure the perceived satisfaction with a process’ output." ;
                  rdfs:label "PerceivedQuality" .


###  http://www.sws.org/sws#Performance
:Performance rdf:type owl:Class ;
             rdfs:label "Performance" .


###  http://www.sws.org/sws#PhysicalDemands
:PhysicalDemands rdf:type owl:Class ;
                 rdfs:subClassOf :Social ;
                 rdfs:comment "Physical demands measurements quantify the physical efforts associated with performing a work task." ;
                 rdfs:label "PhysicalDemands" .


###  http://www.sws.org/sws#PhysicalErgonomics
:PhysicalErgonomics rdf:type owl:Class ;
                    rdfs:subClassOf :Social ;
                    rdfs:comment "Physical ergonomics indicators reflect how a task or process allows workers to maintain correct postures and proper movements." ;
                    rdfs:label "PhysicalErgonomics" .


###  http://www.sws.org/sws#Privacy
:Privacy rdf:type owl:Class ;
         rdfs:subClassOf :Social ;
         rdfs:comment "Privacy indicators measure a system’s capacity to ensure informational privacy." ;
         rdfs:label "Privacy" .


###  http://www.sws.org/sws#ProblemSolving
:ProblemSolving rdf:type owl:Class ;
                rdfs:subClassOf :Social ;
                rdfs:comment "Problem-solving indicators assess the degree of innovativeness required for completing a task." ;
                rdfs:label "ProblemSolving" .


###  http://www.sws.org/sws#Productivity
:Productivity rdf:type owl:Class ;
              rdfs:subClassOf :Operational ;
              rdfs:comment "Productivity metrics measure the efficiency of a production process." ;
              rdfs:label "Productivity" .


###  http://www.sws.org/sws#Quality
:Quality rdf:type owl:Class ;
         rdfs:subClassOf :Operational ;
         rdfs:comment "Quality metrics measure the degree to which a process meets the demanded specifications or requirements." ;
         rdfs:label "Quality" .


###  http://www.sws.org/sws#Reliability
:Reliability rdf:type owl:Class ;
             rdfs:subClassOf :Technical ;
             rdfs:comment "Reliability metrics evaluate whether the AI system behaves according to its design intentions." ;
             rdfs:label "Reliability" .


###  http://www.sws.org/sws#Resource
:Resource rdf:type owl:Class ;
          rdfs:label "Resource" .


###  http://www.sws.org/sws#ReturnOnInvestment
:ReturnOnInvestment rdf:type owl:Class ;
                    rdfs:subClassOf :Operational ;
                    rdfs:comment "Return on investment is an investment’s profitability measure." ;
                    rdfs:label "ReturnOnInvestment" .


###  http://www.sws.org/sws#Robustness
